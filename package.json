{
  "name": "three-debug",
  "version": "0.0.25",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/three-debug.umd.cjs",
  "module": "./dist/main.js",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "import": "./dist/main.js",
      "require": "./dist/main.umd.cjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michealparks/three-debug.git"
  },
  "keywords": [
    "three",
    "three.js",
    "3d",
    "debug",
    "inspect",
    "webgl"
  ],
  "license": "MIT",
  "scripts": {
    "start": "vite --host",
    "build": "vite build && tsc --emitDeclarationOnly",
    "build-playground": "vite build -c vite.playground.config.ts --base=/three-debug/",
    "check": "tsc",
    "lint": "eslint ./src",
    "preview": "vite preview --host",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@0b5vr/tweakpane-plugin-rotation": "^0.1.1",
    "@tweakpane/core": "^1.1.0",
    "@tweakpane/plugin-essentials": "^0.1.5",
    "@types/three": "^0.144.0",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "eslint": "^8.26.0",
    "postcss": "^8.4.18",
    "postprocessing": "^6.29.0",
    "tailwindcss": "^3.2.1",
    "three": "^0.145.0",
    "three-good-godrays": "^0.4.0",
    "three-kit": "github:michealparks/three-kit",
    "tweakpane": "^3.1.0",
    "typescript": "^4.8.4",
    "vite": "^3.1.8"
  }
}

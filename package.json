{
	"name": "three-inspect",
	"version": "0.4.4",
	"type": "module",
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js",
			"import": "./dist/index.js"
		},
		"./vanilla": {
			"types": "./dist/inspector.d.ts",
			"import": "./dist/inspector.js"
		}
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/michealparks/three-inspect.git"
	},
	"keywords": [
		"three",
		"three.js",
		"3d",
		"inspect",
		"debug",
		"webgl",
		"monitoring",
		"threlte",
		"r3f"
	],
	"license": "MIT",
	"scripts": {
		"start": "vite --host --open",
		"build": "vite build && npm run package",
		"package": "svelte-kit sync && svelte-package && npm run package:vanilla && publint",
		"package:vanilla": "vite build --config vite.lib.config.ts && tsc --project tsconfig.lib.json --emitDeclarationOnly",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:format": "prettier --check .",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "eslint ./src",
		"format": "prettier --write .",
		"prettier": "prettier --check \"**/*.{ts,svelte}\"",
		"preview": "vite preview --host",
		"typecheck": "tsc --noEmit",
		"release": "changeset publish"
	},
	"peerDependencies": {
		"three": ">=0.140"
	},
	"dependencies": {
		"@changesets/cli": "^2.27.1",
		"@floating-ui/dom": "^1.6.1",
		"@mdi/js": "^7.4.47",
		"@threlte/core": "^7.0.12",
		"@threlte/extras": "^8.7.1",
		"camera-controls": "^2.7.4",
		"flexible-tree": "^0.1.6",
		"svelte": "^4.2.10",
		"svelte-splitpanes": "^0.8.0",
		"svelte-tweakpane-ui": "^1.2.0",
		"three-perf": "^1.0.10"
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "^3.0.1",
		"@sveltejs/kit": "^2.5.0",
		"@sveltejs/package": "^2.2.6",
		"@sveltejs/vite-plugin-svelte": "^3.0.2",
		"@types/three": "^0.161.2",
		"@typescript-eslint/eslint-plugin": "^6.21.0",
		"@typescript-eslint/parser": "^6.21.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"eslint-plugin-unicorn": "^51.0.1",
		"postcss": "^8.4.35",
		"postprocessing": "^6.34.2",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.1.2",
		"publint": "^0.2.7",
		"simplex-noise": "^4.0.1",
		"svelte-check": "^3.6.4",
		"tailwindcss": "^3.4.1",
		"three": "^0.161.0",
		"tslib": "^2.6.2",
		"typescript": "^5.3.3",
		"vite": "^5.1.0",
		"vite-plugin-glsl": "^1.2.1"
	},
	"svelte": "./dist/index.js"
}

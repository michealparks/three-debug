{
  "name": "three-inspect",
  "version": "0.4.3",
  "type": "module",
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "import": "./dist/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michealparks/three-inspect.git"
  },
  "keywords": [
    "three",
    "three.js",
    "3d",
    "inspect",
    "debug",
    "webgl",
    "monitoring",
    "threlte",
    "r3f"
  ],
  "license": "MIT",
  "scripts": {
    "start": "vite --host --open",
    "build": "vite build",
    "package": "svelte-kit sync && svelte-package && npm run package:vanilla && publint",
    "package:vanilla": "vite build --config vite.lib.config.ts && tsc --project tsconfig.lib.json --emitDeclarationOnly",
    "prepublishOnly": "npm run package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:format": "prettier --check .",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint ./src",
    "format": "prettier --write .",
    "preview": "vite preview --host",
    "typecheck": "tsc --noEmit",
    "release": "changeset publish",
    "test": "vitest"
  },
  "dependencies": {
    "@changesets/cli": "^2.27.1",
    "@floating-ui/dom": "^1.5.4",
    "@kitschpatrol/tweakpane-image-plugin": "^2.0.0",
    "@mdi/js": "^7.4.47",
    "@threlte/core": "^8.0.0-next.3",
    "@threlte/extras": "^9.0.0-next.4",
    "@tweakpane/plugin-camerakit": "^0.3.0",
    "camera-controls": "^2.7.3",
    "estree-walker": "^3.0.3",
    "flexible-tree": "^0.1.6",
    "index-to-position": "^1.0.0",
    "magic-string": "^0.30.10",
    "svelte": "5.0.0-next.106",
    "svelte-splitpanes": "^0.8.0",
    "svelte-tweakpane-ui": "^1.2.3",
    "three": "^0.163.0",
    "three-perf": "^1.0.10",
    "tweakpane": "^4.0.3"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.2.0",
    "@sveltejs/adapter-static": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/package": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@types/three": "^0.160.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.36.0-next.4",
    "eslint-plugin-unicorn": "^50.0.1",
    "immer": "^9.0.21",
    "mitt": "^3.0.1",
    "postcss": "^8.4.33",
    "postprocessing": "^6.35.3",
    "prettier": "^3.2.2",
    "prettier-plugin-svelte": "^3.1.2",
    "publint": "^0.2.7",
    "simplex-noise": "^4.0.1",
    "svelte-check": "^3.6.3",
    "tailwindcss": "^3.4.1",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vite-dev-rpc": "^0.1.4",
    "vite-plugin-glsl": "^1.2.1",
    "vite-plugin-inspect": "^0.8.3",
    "vitest": "^1.4.0"
  },
  "svelte": "./dist/index.js"
}

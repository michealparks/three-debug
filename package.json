{
  "name": "three-inspect",
  "version": "0.3.4",
  "type": "module",
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michealparks/three-inspect.git"
  },
  "keywords": [
    "three",
    "three.js",
    "3d",
    "inspect",
    "debug",
    "webgl",
    "monitoring",
    "threlte",
    "r3f"
  ],
  "license": "MIT",
  "scripts": {
    "start": "vite --host --open",
    "build": "vite build && npm run package",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint ./src",
    "preview": "vite preview --host",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "three": "*"
  },
  "devDependencies": {
    "@0b5vr/tweakpane-plugin-rotation": "^0.2.0",
    "@dimforge/rapier3d-compat": "^0.11.2",
    "@pangenerator/tweakpane-textarea-plugin": "^1.0.4",
    "@sveltejs/adapter-auto": "^2.1.1",
    "@sveltejs/kit": "^1.27.6",
    "@sveltejs/package": "^2.2.3",
    "@threlte/core": "^7.0.2",
    "@threlte/extras": "^7.5.4",
    "@threlte/rapier": "^1.1.3",
    "@tweakpane/core": "^2.0.1",
    "@tweakpane/plugin-essentials": "^0.2.0",
    "@types/three": "^0.159.0",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "camera-controls": "^2.7.3",
    "eslint": "^8.55.0",
    "eslint-plugin-unicorn": "^49.0.0",
    "flexible-tree": "^0.1.6",
    "postcss": "^8.4.32",
    "postprocessing": "^6.33.4",
    "publint": "^0.2.6",
    "simplex-noise": "^4.0.1",
    "svelte": "^4.2.8",
    "svelte-check": "^3.6.2",
    "svelte-splitpanes": "^0.8.0",
    "tailwindcss": "^3.3.5",
    "three": "^0.159.0",
    "trzy": "^0.3.17",
    "tslib": "^2.6.2",
    "tweakpane": "^4.0.1",
    "tweakpane-image-plugin": "^1.1.3",
    "typescript": "^5.3.2",
    "vite": "^5.0.4",
    "vite-plugin-glsl": "^1.2.0"
  },
  "svelte": "./dist/index.js"
}

{
  "name": "three-inspect",
  "version": "0.3.3",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/three-inspect.umd.cjs",
  "module": "./dist/main.js",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "import": "./dist/main.js",
      "require": "./dist/main.umd.cjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michealparks/three-inspect.git"
  },
  "keywords": [
    "three",
    "three.js",
    "3d",
    "inspect",
    "debug",
    "webgl",
    "monitoring",
    "threlte",
    "r3f"
  ],
  "license": "MIT",
  "scripts": {
    "start": "vite -c vite.playground.config.ts --host --open",
    "build": "vite build && tsc --emitDeclarationOnly",
    "build-playground": "vite build -c vite.playground.config.ts --base=/three-inspect/",
    "check": "tsc",
    "lint": "eslint ./src",
    "preview": "vite preview --host",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "three": "*"
  },
  "devDependencies": {
    "@0b5vr/tweakpane-plugin-rotation": "^0.1.1",
    "@pangenerator/tweakpane-textarea-plugin": "^1.0.4",
    "@tweakpane/core": "^1.1.8",
    "@tweakpane/plugin-essentials": "^0.1.8",
    "@types/three": "^0.152.0",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "camera-controls": "^2.3.5",
    "eslint": "^8.41.0",
    "flexible-tree": "^0.1.5",
    "postcss": "^8.4.23",
    "postprocessing": "^6.31.0",
    "simplex-noise": "^4.0.1",
    "tailwindcss": "^3.3.2",
    "three": "0.152.2",
    "three-good-godrays": "^0.4.1",
    "trzy": "^0.0.49",
    "tweakpane": "^3.1.9",
    "tweakpane-image-plugin": "^1.1.3",
    "typescript": "^5.0.4",
    "vite": "^4.3.9",
    "vite-plugin-glsl": "^1.1.2"
  }
}
